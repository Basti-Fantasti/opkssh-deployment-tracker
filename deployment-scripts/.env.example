# opkssh Deployment Configuration
# Copy this file to .env and adjust settings for your environment

# Tracker server URL
TRACKER_URL="http://your-tracker-server:8080"

# Tracker authentication
TRACKER_USER="admin"
TRACKER_PASS="your-password-here"

# Default SSH principal/user (root, pi, ubuntu, etc.)
DEFAULT_PRINCIPAL="root"

# OpenID Provider Configuration
# These settings are used by deploy-opkssh.sh for configuring opkssh
PROVIDER_ISSUER="https://auth.yourdomain.com"
PROVIDER_CLIENT_ID="111222333-abcde-abcde-abcd-abcdef12345"
PROVIDER_EXPIRY="24h"
USER_EMAIL="your-email@host.com"
